generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Character {
  id         String   @id @default(cuid())
  name       String
  occupation String?
  data       Json
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
